# Credenciales de Telegram API
# Obtén estos valores en https://my.telegram.org/apps
TELEGRAM_API_ID=12345678
TELEGRAM_API_HASH=abcdef1234567890abcdef1234567890

# Número de teléfono de la cuenta de Telegram
# Debe estar en formato internacional con +
TELEGRAM_PHONE_NUMBER=+5491112345678

# Nombre del archivo de sesión (sin extensión .session)
SESSION_NAME=telegram_session

# Puerto del servidor
PORT=5000
